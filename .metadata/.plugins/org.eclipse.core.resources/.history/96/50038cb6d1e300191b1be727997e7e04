#3-7
PROGRAM tres7
    IMPLICIT NONE
    REAL::x,y,fun

    WRITE (*,*) 'Enter the values of x and y'
    READ (*,*) x,y

    ge0: IF (x>=0) THEN
        gy: IF (y>=0) THEN
            fun=x+y
            ELSE IF (y<0) THEN gy
            fun=x+y**2
            END IF gy
         ELSE IF (x<0) THEN ge0
         gy2: IF (y>=0) THEN
              fun=x**2+y
              ELSE IF (y<0) THEN gy2
              fun=x**2+y**2
              END IF gy2
         END IF ge0
     WRITE (*,*) 'El valor de la función f(x,y) es ',fun

END PROGRAM tres7

